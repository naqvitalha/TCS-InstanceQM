{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "max-line-length": {
      "options": [
        160
      ]
    },
    "typedef": [
      true,
      "call-signature",
      "property-declaration"
    ],
    "typeof-compare": true,
    "interface-name": false,
    "variable-name": false,
    "comment-format": false,
    "no-bitwise": false,
    "member-access": true,
    "max-classes-per-file": [
      2
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false
  }
}